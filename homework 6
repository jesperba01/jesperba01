#task 1
sentence = input("input me something:")

print({item: sentence.count(item) for item in sentence.split('')[:-1]})

#task 2
#exampel

stock = {
    "banana": 6,
    "apple": 0,
    "orange": 32,
    "pear": 15
}
prices = {
    "banana": 4,
    "apple": 2,
    "orange": 1.5,
    "pear": 3
}

value = sum(prices[v] * stock[v] for v in prices)


print(value)

#task 3

num_list = [2, 6, 23, 36, 45, 67, 77, 89, 92, 99]
for i, j in enumerate(num_list):
    num_list[i] = (i, j)

print(num_list)
